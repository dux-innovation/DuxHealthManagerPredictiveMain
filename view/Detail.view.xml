<?xml version="1.0" encoding="UTF-8" ?>
<mvc:View
    controllerName="dux.view.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">

    <Page
        showNavButton="false"
        title="{i18n>SecondPageTitle}">
        <ObjectHeader
            title="{patient/name}"
            number="{ABV}%"
            numberUnit="ABV">
            <attributes>
               <ObjectAttribute text="{doctor/id}" /> 
               <ObjectAttribute text="{doctor/name}" /> 
               <ObjectAttribute text="{doctor/crm}" /> 
               <ObjectAttribute text="{patient/id}" /> 
               <ObjectAttribute text="{patient/name}" /> 
            </attributes>
        </ObjectHeader>
    </Page>

</mvc:View>
